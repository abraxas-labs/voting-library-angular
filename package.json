    "eslint-plugin-license-header": "0.6.1",
    "google-protobuf": "3.21.2",
    "grpc-web": "1.5.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "ng-packagr": "18.1.0",
    "prettier": "3.3.2",
    "prettier-stylelint": "0.4.2",
    "stylelint": "16.6.1",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-scss": "6.4.0",
    "typescript": "5.4.5"
